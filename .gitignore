# .gitignore
# Create .gitignore to protect sensitive files
cat > .gitignore << 'EOF'
# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Google Cloud credentials
service-account-key.json
*.json

# Python
__pycache__/
*.py[cod]
*$py.class
venv/
env/

# IDE
.vscode/
.idea/

# OS
.DS_Store
Thumbs.db

# Logs
*.log
EOF

.pytest_cache/